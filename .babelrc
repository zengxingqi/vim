{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-1",
    "stage-2",
    [
      "es2015",
      { "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 12 versions", "not ie <= 10"]
        }
      }
    ]
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime", "syntax-dynamic-import"]
}
